package parcialViajeEscolar;
public class Alumno {
    private int dni;
    private String nombre;
    private int poliza;
    private String obra;

    public Alumno(int dni, String nombre, int poliza, String obra) {
        this.dni = dni;
        this.nombre = nombre;
        this.poliza = poliza;
        this.obra = obra;
    }

    public int getDni() {
        return dni;
    }

    public String getNombre() {
        return nombre;
    }

    public int getPoliza() {
        return poliza;
    }

    public String getObra() {
        return obra;
    }

    public void setPoliza(int poliza) {
        this.poliza = poliza;
    }
    
    
}
