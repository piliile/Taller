
package parciales.parcialturnoMAÑANAtema2_12_5_2025;

public class Nadador extends Grupo {
    private String nombreBañero;
    private double sueldoBañero;

    public Nadador(String nombreBañero, double sueldoBañero, String nombre, double sueldoInstructor, double costoInscripcion, int N) {
        super(nombre, sueldoInstructor, costoInscripcion, N);
        this.nombreBañero = nombreBañero;
        this.sueldoBañero = sueldoBañero;
    }
   public boolean agregarChico(Chico C){
        boolean exito = false;
        if (C.isSabeNadar()){
            super.agregoChico(C);
            exito = true;
        }
        return exito;
    }
    
    
    public double descuentoGrupo(){
        return this.sueldoBañero;
    }
}
