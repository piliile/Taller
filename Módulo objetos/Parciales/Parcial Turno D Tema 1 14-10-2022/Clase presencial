package parciales.parcialturnoDtema1_14_10_2022;

public class Presencial extends Curso{
    private int aula;

    public Presencial(int aula, int añoCursada, int N) {
        super(añoCursada, N);
        this.aula = aula;
    }
    
    public boolean puedeRendir(Alumno unAlumno){
        return unAlumno.getAsistencias() >= 3;
    }
}
