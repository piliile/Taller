package parcialturnoDtema1_10_10_2023;

public class Vehiculo {
    private String patente;
    private int cantidadHorasEstacionado;
    private String marca;
    private String modelo;

    public Vehiculo(String patente, int cantidadHorasEstacionado, String marca, String modelo) {
        this.patente = patente;
        this.cantidadHorasEstacionado = cantidadHorasEstacionado;
        this.marca = marca;
        this.modelo = modelo;
    }

    public String getMarca() {
        return marca;
    }
    
    public String toString(){
        return "Patente: " + this.patente + ". Marca: " + this.marca + ". Modelo: " + this.modelo + ". Horas estacionado: " 
                + this.cantidadHorasEstacionado + ".";
        
    }

    public int getCantidadHorasEstacionado() {
        return cantidadHorasEstacionado;
    }
    
    
    
}
